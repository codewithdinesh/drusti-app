<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="darkTheme" parent="Theme.Material3.Dark.NoActionBar">
        <item name="colorPrimary">#545D8A</item>
        <item name="colorPrimaryVariant">#B3B1B1</item>
        <item name="colorOnPrimary">#B2FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#C0CA33</item>
        <item name="colorSecondaryVariant">#808538</item>
        <item name="colorOnSecondary">#D6D6D6</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <item name="android:windowLightStatusBar" tools:targetApi="l">true</item>
        <item name="android:navigationBarColor" tools:targetApi="l">?attr/colorPrimary</item>
    </style>
</resources>